    "@types/escape-html": "0.0.20",
    "@types/file-type": "^10.9.1",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/spark-md5": "^3.0.1",
    "@types/traverse": "^0.6.32",
    "css-loader": "^3.1.0",
    "escape-html": "^1.0.3",
    "eslint": "^7.2.0",
